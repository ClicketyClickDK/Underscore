    CALL "%~dp0\_DEBUG"
    CALL "%~dp0\_Getopt" %*&IF ERRORLEVEL 1 EXIT /B 1

::ENDLOCAL

    SET UTC=%~1
    IF NOT DEFINED UTC CALL _utc
    SET /A Month=10%UTC:~5,2% %% 100 - 1
    ::SET /a Month=1%month% - 101
    SET /A QuarterOfYear=(%Month% / 3) + 1
    SET QuarterOfYear
    ENDLOCAL&SET QuarterOfYear=%QuarterOfYear%
GOTO :EOF    

