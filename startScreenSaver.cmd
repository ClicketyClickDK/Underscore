    CALL "%~dp0\_DEBUG"
    CALL "%~dp0\_Getopt" %*&IF ERRORLEVEL 1 EXIT /B 1

::ENDLOCAL

:Main
    CALL :ScreenSaver
    TimeOut 5
    ::CALL :LockWorkstation
GOTO :EOF

:ScreenSaver
    :: URL http://boards.straightdope.com/sdmb/showthread.php?t=612612
    ECHO Start screen saver..
    @START /MIN %windir%\system32\PhotoScreensaver.scr -s
GOTO :EOF

